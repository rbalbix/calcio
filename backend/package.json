{
  "name": "backend",
  "version": "1.0.0",
  "engines": {
    "node": "13.11.0"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
    "start:ts": "ts-node src/server.ts",
    "build": "rm -rf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored && rm -rf dist/node_modules dist/__tests__",
    "uploads": "cp -r ./uploads/ ./dist/uploads/",
    "start": "node dist/server.js",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seeder.ts",
    "seed:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/database/seeds/seeder.ts",
    "test": "cross-env NODE_ENV=test jest --watch --detectOpenHandles"
  },
  "dependencies": {
    "celebrate": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.0",
    "http-status-codes": "^1.4.0",
    "log4js": "^6.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.9",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.0",
    "@types/mongoose": "^5.7.24",
    "@types/morgan": "^1.9.1",
    "@types/supertest": "^2.0.9",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.47",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
